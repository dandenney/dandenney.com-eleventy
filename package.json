{
  "name": "dandenney.com-11ty",
  "version": "1.0.0",
  "description": "This thing holds a series of words that I've spewed onto the internet since 2012. It's mainly so that I have a place to learn and play with front-end development, but also to keep my Google juice up against the guy with my name that created an airplane. Seriously, there's other Dan Denneys?",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "run-s build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "tailwindcss -i src/css/tailwind.css -o _site/css/tailwind.css --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "tailwindcss -i src/css/tailwind.css -o _site/css/tailwind.css --postcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dandenney/dandenney.com-11ty.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dandenney/dandenney.com-11ty/issues"
  },
  "homepage": "https://github.com/dandenney/dandenney.com-11ty#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^3.3.2"
