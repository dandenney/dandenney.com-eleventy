<ol reversed class="postlist" style="counter-reset: start-from {{ (tinkeringslistCounter or tinkeringslist.length) + 1 }}">
  {% for tinkering in tinkeringslist | reverse %}
  <li class="postlist-item{% if tinkering.url == url %} postlist-item-active{% endif %}">
    <time class="postlist-date" datetime="{{ tinkering.date | htmlDateString }}">
      <span class="postlist-month">{{ tinkering.date | postMonth }}</span>
      <span class="postlist-day">{{ tinkering.date | postDay }}</span>
      <span class="postlist-year">{{ tinkering.date | postYear }}</span>
    </time>
    <div class="postlist-meta">
      <a href="{{ tinkering.url | url }}" class="postlist-link">
        {% if tinkering.data.title %}
          {{ tinkering.data.title }}
        {% else %}
          <code>{{ tinkering.url }}</code>
        {% endif %}
      </a>
    </div>
  </li>
  {% endfor %}
</ol>
